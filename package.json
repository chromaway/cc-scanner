  "files": [
    "bin",
    "src",
    "LICENSE",
    "README.md"
  ],
  "main": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaway/cc-scanner"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run compile",
    "clean": "rm -f lib/*.js bin/cli.js",
    "compile": "npm run compile:bin && npm run compile:src",
    "compile:bin": "babel bin/cli.es6.js -o bin/cli.js --optional runtime -s inline",
    "compile:src": "mkdir -p lib && babel src -d lib --optional runtime -s inline",
    "lint": "eslint bin/cli.es6.js src",
    "watch": "npm run watch:bin & npm run watch:src",
    "watch:bin": "babel bin/cli.es6.js -o bin/cli.js --optional runtime -s inline -w",
    "watch:src": "mkdir -p lib && babel src -d lib --optional runtime -s inline -w"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "babel-runtime": "^5.8.20",
    "bitcoind-rpc-client": "^0.1.0",
    "bitcore": "^0.12.15",
    "body": "^5.1.0",
    "coloredcoinjs-lib": "git://github.com/fanatid/coloredcoinjs-lib",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "js-yaml": "^3.3.1",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "odd-storage": "^0.3.1",
    "progress": "^1.1.8",
    "ready-mixin": "^0.1.0",
    "source-map-support": "^0.3.2",
    "yargs": "^3.19.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "eslint": "^1.1.0",
    "eslint-config-standard": "^4.1.0",
    "eslint-plugin-standard": "^1.2.0"
  },
  "private": true
}
